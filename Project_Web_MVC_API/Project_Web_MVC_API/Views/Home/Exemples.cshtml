<section id="examples" class="text-center">
    <h2 class="mb-4 font-weight-bold">Products</h2>
    <!--Grid-->
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-lg-4 col-md-6 mb-4">
                <div class="view overlay hm-white-strong z-depth-1-half">
                    <img src="~/img/Background/@item.Image" class="img-fluid" alt="" />
                    <div class="mask"></div>
                </div>
                <h4 class="my-4 font-weight-bold">@item.Name</h4>
                <p class="grey-text">@item.Description</p>
            </div>
        }
    </div>
</section>

